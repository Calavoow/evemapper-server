{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "strategies": [
      "jwt",
      "evesso"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "http://localhost:3030",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "facebook": {
      "clientID": "your facebook client id",
      "clientSecret": "your facebook client secret",
      "successRedirect": "/",
      "scope": [
        "public_profile",
        "email"
      ],
      "profileFields": [
        "id",
        "displayName",
        "first_name",
        "last_name",
        "email",
        "gender",
        "profileUrl",
        "birthday",
        "picture",
        "permissions"
      ]
    },
    "evesso": {
      "callbackURL": "http://localhost:3030/auth/evesso",
      "scope": [
        "esi-location.read_location.v1",
        "esi-location.read_ship_type.v1",
        "esi-ui.write_waypoint.v1",
        "esi-location.read_online.v1"
      ],
      "idField": "characterId",
      "entity": "characters",
      "service": "characters"
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    }
  },
  "postgres": "postgres://evemapper:@localhost:5432/evemapper"
}
